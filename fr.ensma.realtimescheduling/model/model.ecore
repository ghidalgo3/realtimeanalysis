    <eStructuralFeatures xsi:type="ecore:EAttribute" name="availabilityFactor" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble" defaultValueLiteral="0"
        derived="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
        <details key="derivation" value="--check for divide by zero!&#xA;&#x9;&#x9;&#x9;&#x9;if (period &lt;> 0)&#xA;&#x9;&#x9;&#x9;&#x9;then executionIntervals&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;->collect(i : Interval | i.end - i.start)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;->sum() / period&#xA;&#x9;&#x9;&#x9;&#x9;else 0.0&#xA;&#x9;&#x9;&#x9;&#x9;endif"/>
      </eAnnotations>
    </eStructuralFeatures>
