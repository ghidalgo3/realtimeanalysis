  <eClassifiers xsi:type="ecore:EClass" name="Partition">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="PositivePeriod AvailibilityFactorLessThanOrEqualToOne PeriodSpansIntervals NonOverlappingIntervals"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="PositivePeriod" value="period > 0"/>
      <details key="AvailibilityFactorLessThanOrEqualToOne" value="availabilityFactor &lt;= 1"/>
      <details key="PeriodSpansIntervals" value="let sortedIntervals : Sequence(Interval) = executionIntervals&#xA;&#x9;&#x9;&#x9;->sortedBy(start)&#xA;&#x9;&#x9;in if (sortedIntervals&#xA;&#x9;&#x9;&#x9;->size() > 1)&#xA;&#x9;&#x9;then sortedIntervals&#xA;&#x9;&#x9;&#x9;->last().end &lt;= period&#xA;&#x9;&#x9;else true&#xA;&#x9;&#x9;endif"/>
      <details key="NonOverlappingIntervals" value="if (executionIntervals&#xA;&#x9;&#x9;&#x9;->size() &lt;= 1)&#xA;&#x9;&#x9;then true -- Nothing can overlap if there is only one or none!&#xA;&#x9;&#x9;else&#xA;&#x9;&#x9;let sortedIntervals : Sequence(Interval) = executionIntervals&#xA;&#x9;&#x9;&#x9;->sortedBy(i : Interval | i.start)&#xA;&#x9;&#x9;in sortedIntervals&#xA;&#x9;&#x9;&#x9;->subSequence(1, sortedIntervals&#xA;&#x9;&#x9;&#x9;&#x9;->size() - 1)&#xA;&#x9;&#x9;&#x9;->forAll(i : Interval | i.end &lt;= sortedIntervals&#xA;&#x9;&#x9;&#x9;&#x9;->at(1 + sortedIntervals&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;->indexOf(i)).start)&#xA;&#x9;&#x9;endif"/>
    </eAnnotations>
